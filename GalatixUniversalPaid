-- LOCAL PLAYER
local Player = Window:NewTab("Local Player")
local PlayerSection = Player:NewSection("Player Config")

PlayerSection:NewSlider("Speed Slider", "Change your speed with this slider", 150, 16, function(s)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)


PlayerSection:NewSlider("JumpPower Slider", "Change your jumppower with this slider", 225, 50, function(s)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)

PlayerSection:NewButton("FE Flip", "Makes you very athletic", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/BroSlimeDevGamerHD/BroSlimeDevGamerHD/main/FeFlip"))()
end)


PlayerSection:NewButton("Reset WS/JP", "Reset Jumpppower and Walkspeed", function()
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 50
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = 16
end)



-- Other
local Other = Window:NewTab("Other")
local OtherSection = Other:NewSection("Main")


OtherSection:NewToggle("SpinBot", "Makes you spin rapidly", function(state)
    if state then
        local speed = 50

local plr = game:GetService("Players").LocalPlayer
repeat task.wait() until plr.Character
local humRoot = plr.Character:WaitForChild("HumanoidRootPart")
plr.Character:WaitForChild("Humanoid").AutoRotate = false
local velocity = Instance.new("AngularVelocity")
velocity.Attachment0 = humRoot:WaitForChild("RootAttachment")
velocity.MaxTorque = math.huge
velocity.AngularVelocity = Vector3.new(0, speed, 0)
velocity.Parent = humRoot
velocity.Name = "Spinbot" 



    else


        local speed = 0

local plr = game:GetService("Players").LocalPlayer
repeat task.wait() until plr.Character
local humRoot = plr.Character:WaitForChild("HumanoidRootPart")
plr.Character:WaitForChild("Humanoid").AutoRotate = false
local velocity = Instance.new("AngularVelocity")
velocity.Attachment0 = humRoot:WaitForChild("RootAttachment")
velocity.MaxTorque = math.huge
velocity.AngularVelocity = Vector3.new(0, speed, 0)
velocity.Parent = humRoot
velocity.Name = "Spinbot" 
    end
end)

OtherSection:NewButton("Chat Spoofer", "Used to impersonate other players", function()
    loadstring(game:HttpGet("loadstring(game:HttpGet(('https://pastebin.com/raw/djBfk8Li'),true))()"))()
end)

