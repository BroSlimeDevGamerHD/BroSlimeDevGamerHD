print("Galatix Universal V.0.1 Paid has loaded")
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Galatix Universal Paid V.0.1", "BloodTheme")
local Tab = Window:NewTab("Main")
local MainSection = Tab:NewSection("Main")
MainSection:NewSlider("Speed Slider", "Change your speed with this slider", 150, 16, function(s)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)
MainSection:NewSlider("JumpPower Slider", "Change your jumppower with this slider", 225, 50, function(s)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)
MainSection:NewButton("FE Flip", "Makes you very athletic", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/BroSlimeDevGamerHD/BroSlimeDevGamerHD/main/FeFlip"))()
end)
MainSection:NewButton("Infinite Yield Loader", "Loads Infinite Yield Admin", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end)
MainSection:NewButton("CMD-X Admin Loader", "Loads CMD-X Admin Commands", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/CMD-X/CMD-X/master/Source"))()
end)
local ShooterTab = Window:NewTab("Shooter Scripts")
local AimbotSection = Tab:NewSection("Main")

MainSection:NewButton("Universal ESP", "Lets you see people through parts", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/Lucasfin000/SpaceHub/main/UESP'))()
end)  
MainSection:NewButton("Hitbox Expander", "Makes players hitboxes much larger", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/BroSlimeDevGamerHD/BroSlimeDevGamerHD/main/HitboxExpander'))()
end)  
AimbotSection:NewButton("Universal Aimbot (No FOV)", "Lock onto players without a FOV circle", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/Exunys/Aimbot-Script/main/Aimbot%20Script%20(Without%20FOV).lua'))()
end)  
AimbotSection:NewButton("Universal Aimbot (FOV)", "Lock on to players with FOV circle", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/Exunys/Aimbot-Script/main/Aimbot%20Script.lua'))()
end)  
AimbotSection:NewButton("Silent Aim", "Self Explanitory", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/Averiias/Universal-SilentAim/main/main.lua'))()
end)  
